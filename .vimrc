""execute pathogen#infect()
syntax on
filetype plugin indent on
colorscheme molokai

:set autochdir
:set tags=./tags,tags;$HOME
:set number
:set shell=/bin/zsh
:set guifont=Menlo:h14
:set nocompatible
:set modelines=0
:set tabstop=2
:set shiftwidth=2
:set softtabstop=2
:set expandtab
:set encoding=utf-8
:set scrolloff=3
:set autoindent
:set showmode
:set showcmd
:set visualbell
:set ttyfast
:set ruler
:set backspace=indent,eol,start
:set laststatus=2
:set ignorecase
:set smartcase
:set incsearch
:set showmatch
:set hlsearch
:set wrap
:set textwidth=0
:set formatoptions=qrn1
inoremap <F1> <ESC>
nnoremap <F1> <ESC>
vnoremap <F1> <ESC>
